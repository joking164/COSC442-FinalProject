                  <generalization xmi:id="_kk0boVIjEeibjuLm5_lcNQ" general="_kED38FIjEeibjuLm5_lcNQ"/>
                  <ownedAttribute xmi:id="_knHhsFIjEeibjuLm5_lcNQ" name="id" isLeaf="true" isStatic="true" isReadOnly="true">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                    <defaultValue xmi:type="uml:LiteralString" xmi:id="_knHhsVIjEeibjuLm5_lcNQ" value="&quot;changeAction&quot;"/>
