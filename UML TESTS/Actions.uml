                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_knZ1lVIjEeibjuLm5_lcNQ" name="ChangeAction" specification="_klpiEFIjEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_knZ1llIjEeibjuLm5_lcNQ" name="freeColClient" type="_kkuVAVIjEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_knZ1l1IjEeibjuLm5_lcNQ" type="_kCa5MFIjEeibjuLm5_lcNQ" direction="return"/>
