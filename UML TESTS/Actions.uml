                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_knZOg1IjEeibjuLm5_lcNQ" name="addSpecificationActions" specification="_klmewFIjEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_knZOhFIjEeibjuLm5_lcNQ" name="spec" type="_kkwxQVIjEeibjuLm5_lcNQ"/>
                    <language>java</language>
                    <body>// Initialize ImprovementActions&#xD;
     for (TileImprovementType type : spec.getTileImprovementTypeList()) {&#xD;
