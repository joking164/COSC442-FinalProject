                <ownedParameter xmi:id="_9K3PN1IhEeibjuLm5_lcNQ" name="scale">
                  <eAnnotations xmi:id="_9K3POFIhEeibjuLm5_lcNQ" source="http://www.eclipse.org/uml2/2.0.0/UML">
