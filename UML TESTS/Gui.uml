                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_FWUYRFIjEeibjuLm5_lcNQ" name="stopScrollIfScrollIsActive" specification="_EqEWQFIjEeibjuLm5_lcNQ">
