                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_FW5AFlIjEeibjuLm5_lcNQ" name="getObjectImage" specification="_FT7yolIjEeibjuLm5_lcNQ">
                  <ownedParameter xmi:id="_FW5AF1IjEeibjuLm5_lcNQ" name="display" type="_FN6DIVIjEeibjuLm5_lcNQ"/>
                  <ownedParameter xmi:id="_FW5AGFIjEeibjuLm5_lcNQ" name="scale">
