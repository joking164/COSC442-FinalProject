                <ownedAttribute xmi:id="_FVlYcFIjEeibjuLm5_lcNQ" name="tileHeight" visibility="private">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                </ownedAttribute>
                <ownedAttribute xmi:id="_FVlYc1IjEeibjuLm5_lcNQ" name="corners" visibility="private" isLeaf="true" type="_FQakllIjEeibjuLm5_lcNQ" isReadOnly="true" association="_FYFS0VIjEeibjuLm5_lcNQ">
