                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#float"/>
                  </ownedParameter>
                  <language>java</language>
                  <body>float pixelSize;&#xD;
     switch(fontSize) {&#xD;
         default:&#xD;
             logger.warning(&quot;Unknown FontSize&quot;);&#xD;
         case TINY:&#xD;
             pixelSize = 12f;&#xD;
             break;&#xD;
         case SMALLER:&#xD;
             pixelSize = 16f;&#xD;
             break;&#xD;
         case SMALL:&#xD;
             pixelSize = 24f;&#xD;
             break;&#xD;
         case MEDIUM:&#xD;
             pixelSize = 36f;&#xD;
             break;&#xD;
         case BIG:&#xD;
             pixelSize = 48f;&#xD;
     }&#xD;
     return pixelSize * scaleFactor;&#xD;
 </body>
                </ownedBehavior>
