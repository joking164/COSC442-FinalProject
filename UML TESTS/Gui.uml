                  </ownedParameter>
                  <language>java</language>
                  <body>String fontName;&#xD;
     switch(fontType) {&#xD;
         default:&#xD;
             logger.warning(&quot;Unknown FontType&quot;);&#xD;
         case NORMAL:&#xD;
             fontName = (mainFont != null) ? null : &quot;font.normal&quot;;&#xD;
             break;&#xD;
         case SIMPLE:&#xD;
             fontName = &quot;font.simple&quot;;&#xD;
             break;&#xD;
         case HEADER:&#xD;
             fontName = &quot;font.header&quot;;&#xD;
     }&#xD;
     return fontName;&#xD;
 </body>
