                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_FXDYKVIjEeibjuLm5_lcNQ" name="repositionMapIfNeeded" specification="_Eu2voVIjEeibjuLm5_lcNQ">
                  <language>java</language>
                  <body>if (bottomRow &lt; 0 &amp;&amp; focus != null) positionMap(focus);&#xD;
 </body>
