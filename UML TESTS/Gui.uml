                  <ownedParameter xmi:id="_FWbGB1IjEeibjuLm5_lcNQ" name="freeColDialog" type="_FRPD8FIjEeibjuLm5_lcNQ">
                    <eAnnotations xmi:id="_FWbGCFIjEeibjuLm5_lcNQ" source="http://www.eclipse.org/uml2/2.0.0/UML">
                      <details xmi:id="_FWbGCVIjEeibjuLm5_lcNQ" key="final"/>
