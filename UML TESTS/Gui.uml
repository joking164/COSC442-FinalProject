                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_FXIQklIjEeibjuLm5_lcNQ" name="run" specification="_EvTbkFIjEeibjuLm5_lcNQ">
                  <language>java</language>
                  <body>while (direction != null) {&#xD;
         try {&#xD;
             SwingUtilities.invokeAndWait(() -> {&#xD;
                     if (!canvas.scrollMap(direction)) direction = null;&#xD;
                 });&#xD;
         } catch (InvocationTargetException e) {&#xD;
             logger.log(Level.WARNING, &quot;Scroll thread caught error&quot;, e);&#xD;
             break;&#xD;
         } catch (InterruptedException e) {&#xD;
             break; // It is normal for AbstractCanvasListener to interrupt.&#xD;
         }&#xD;
&#xD;
         try {&#xD;
             sleep(SCROLL_DELAY);&#xD;
         } catch (InterruptedException e) {&#xD;
             break;&#xD;
         }&#xD;
     }&#xD;
 </body>
                </ownedBehavior>
