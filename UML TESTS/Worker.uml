            <packagedElement xmi:type="uml:Class" xmi:id="_BjCwYFIiEeibjuLm5_lcNQ" name="Worker" isLeaf="true">
              <ownedComment xmi:id="_BkUiwlIiEeibjuLm5_lcNQ">
                <body>The Worker Thread executes jobs one after another.  The thread&#xD;
manages a queue where new jobs can be enqueued.  The jobs are&#xD;
processed synchronously by the worker.</body>
              </ownedComment>
              <generalization xmi:id="_BkQRUFIiEeibjuLm5_lcNQ" general="_BkN1EVIiEeibjuLm5_lcNQ"/>
              <ownedAttribute xmi:id="_BkQ4YFIiEeibjuLm5_lcNQ" name="jobList" visibility="private" isLeaf="true" type="_BkI8kFIiEeibjuLm5_lcNQ" isReadOnly="true" association="_BkVw4FIiEeibjuLm5_lcNQ"/>
              <ownedAttribute xmi:id="_BkSGgVIiEeibjuLm5_lcNQ" name="stopRunning" visibility="private">
                <eAnnotations xmi:id="_BkStkFIiEeibjuLm5_lcNQ" source="http://www.eclipse.org/uml2/2.0.0/UML">
                  <details xmi:id="_BkStkVIiEeibjuLm5_lcNQ" key="volatile"/>
                </eAnnotations>
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
              </ownedAttribute>
              <ownedAttribute xmi:id="_BkStk1IiEeibjuLm5_lcNQ" name="logger" visibility="private" isLeaf="true" isStatic="true" type="_BkKKsVIiEeibjuLm5_lcNQ" isReadOnly="true" association="_BkWX8VIiEeibjuLm5_lcNQ">
                <defaultValue xmi:type="uml:LiteralString" xmi:id="_BkTUoFIiEeibjuLm5_lcNQ" value="Logger.getLogger(Worker.class.getName())"/>
              </ownedAttribute>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_BkT7sFIiEeibjuLm5_lcNQ" name="Worker" specification="_BjI3AFIiEeibjuLm5_lcNQ">
                <ownedParameter xmi:id="_BkT7sVIiEeibjuLm5_lcNQ" type="_BjCwYFIiEeibjuLm5_lcNQ" direction="return"/>
                <language>java</language>
                <body>super(FreeCol.CLIENT_THREAD+&quot;Worker&quot;);&#xD;
     jobList = new LinkedBlockingQueue&lt;>();&#xD;
     stopRunning = false;&#xD;
 </body>
              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_BkT7slIiEeibjuLm5_lcNQ" name="run" specification="_BjKFIFIiEeibjuLm5_lcNQ">
                <language>java</language>
                <body>while (!stopRunning) {&#xD;
         try {&#xD;
             // run the next waiting job&#xD;
             Runnable job = jobList.take();&#xD;
             try {&#xD;
                 job.run();&#xD;
             } catch (Exception e) {&#xD;
                 logger.log(Level.SEVERE, &quot;Worker task failed!&quot;, e);&#xD;
             }&#xD;
         } catch (InterruptedException e) {&#xD;
             logger.log(Level.INFO, &quot;Worker interrupted, aborting!&quot;, e);&#xD;
         }&#xD;
     }&#xD;
 </body>
              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_BkT7s1IiEeibjuLm5_lcNQ" name="schedule" specification="_BkPqQVIiEeibjuLm5_lcNQ">
                <ownedParameter xmi:id="_BkUiwFIiEeibjuLm5_lcNQ" name="job" type="_BkPDMFIiEeibjuLm5_lcNQ"/>
                <language>java</language>
                <body>jobList.add(job);&#xD;
 </body>
              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_BkUiwVIiEeibjuLm5_lcNQ" name="askToStop" specification="_BjNvgFIiEeibjuLm5_lcNQ">
                <language>java</language>
                <body>stopRunning = true;&#xD;
     this.interrupt();&#xD;
 </body>
              </ownedBehavior>
