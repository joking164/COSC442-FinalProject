                  <ownedAttribute xmi:id="_OU6OjlIkEeibjuLm5_lcNQ" name="destinationTile" visibility="private" isLeaf="true" type="_OUc7clIkEeibjuLm5_lcNQ" isReadOnly="true" association="_OVL7RFIkEeibjuLm5_lcNQ"/>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_OVD_c1IkEeibjuLm5_lcNQ" name="UnitMoveAnimation" specification="_OUv2iVIkEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_OVD_dFIkEeibjuLm5_lcNQ" name="freeColClient" type="_OUc7cFIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_OVD_dVIkEeibjuLm5_lcNQ" name="unit" type="_OUc7dFIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_OVD_dlIkEeibjuLm5_lcNQ" name="sourceTile" type="_OUc7clIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_OVD_d1IkEeibjuLm5_lcNQ" name="destinationTile" type="_OUc7clIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_OVD_eFIkEeibjuLm5_lcNQ" type="_OSo9kFIkEeibjuLm5_lcNQ" direction="return"/>
