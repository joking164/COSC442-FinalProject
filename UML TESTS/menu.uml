                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Wu4xIFIkEeibjuLm5_lcNQ" name="DebugMenu" specification="_WufvkFIkEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_Wu4xIVIkEeibjuLm5_lcNQ" name="freeColClient" type="_Wt1BMVIkEeibjuLm5_lcNQ"/>
