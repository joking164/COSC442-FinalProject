                    <ownedParameter xmi:id="_9jXufVIkEeibjuLm5_lcNQ" name="comp" type="_9Rvy8VIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_9jXuflIkEeibjuLm5_lcNQ" type="_9RqTY1IkEeibjuLm5_lcNQ" direction="return"/>
