                  <ownedAttribute xmi:id="_9c-LhFIkEeibjuLm5_lcNQ" name="miniMapSkin" visibility="private" isLeaf="true" type="_9RhwgVIkEeibjuLm5_lcNQ" isReadOnly="true" association="_9wsYwFIkEeibjuLm5_lcNQ"/>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9jUrIFIkEeibjuLm5_lcNQ" name="CornerMapControls" specification="_9YTt8FIkEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_9jUrIVIkEeibjuLm5_lcNQ" name="freeColClient" type="_9Rla4VIkEeibjuLm5_lcNQ">
                      <eAnnotations xmi:id="_9jUrIlIkEeibjuLm5_lcNQ" source="http://www.eclipse.org/uml2/2.0.0/UML">
