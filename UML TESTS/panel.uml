                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9jqpblIkEeibjuLm5_lcNQ" name="getUnionRhombus" specification="_6P06ElIkEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_9jqpb1IkEeibjuLm5_lcNQ" type="_9UQ7cVIkEeibjuLm5_lcNQ" direction="return"/>
                    <language>java</language>
                    <body>GeneralPath unionPath = new GeneralPath();&#xD;
&#xD;
     int count = 1;&#xD;
     int square = 1;&#xD;
     while (square &lt; stars) {&#xD;
         count++;&#xD;
         square = count * count;&#xD;
     }&#xD;
     int rows = stars / count;&#xD;
     if (rows * count &lt; stars) rows++;&#xD;
     int starCount = 0;&#xD;
     double a = WIDTH / 2 - BEND_X;&#xD;
     double b = HEIGHT / 2 - BEND_Y;&#xD;
&#xD;
     if (stars &lt; 14) {&#xD;
         double c = Math.sqrt(a * a + b * b);&#xD;
         double r = (a * b) / c;&#xD;
         double radius = 0.6 * r;&#xD;
         unionPath = getCircleOfStars(radius);&#xD;
         center(unionPath, WIDTH / 2, HEIGHT / 2);&#xD;
     } else {&#xD;
         double dx = a / count;&#xD;
         double dy = b / count;&#xD;
         double dx1 = a / rows;&#xD;
         double dy1 = b / rows;&#xD;
         outer: for (int index = 0; index &lt; rows; index++) {&#xD;
             double x = BEND_X + dx + index * dx1;&#xD;
             double y = HEIGHT / 2 - index * dy1;&#xD;
             for (int star = 0; star &lt; count; star++) {&#xD;
                 unionPath.append(getStar(x, y), false);&#xD;
                 starCount++;&#xD;
                 if (starCount == stars) {&#xD;
                     break outer;&#xD;
                 } else {&#xD;
                     x += dx;&#xD;
                     y += dy;&#xD;
                 }&#xD;
             }&#xD;
         }&#xD;
     }&#xD;
&#xD;
     return unionPath;&#xD;
 </body>
                  </ownedBehavior>
