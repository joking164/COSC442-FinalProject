     tableScroll.getViewport().setOpaque(false);&#xD;
     tableScroll.getColumnHeader().setOpaque(false);&#xD;
&#xD;
     connect.setActionCommand(String.valueOf(CONNECT));&#xD;
     connect.addActionListener(this);&#xD;
