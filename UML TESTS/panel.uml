                  <ownedAttribute xmi:id="_9dE5MVIkEeibjuLm5_lcNQ" name="parentPanel" visibility="private" isLeaf="true" type="_6QQ-8FIkEeibjuLm5_lcNQ" isReadOnly="true" association="_9xlwoVIkEeibjuLm5_lcNQ"/>
