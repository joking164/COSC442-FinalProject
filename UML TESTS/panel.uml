                  <ownedAttribute xmi:id="_9cV5Y1IkEeibjuLm5_lcNQ" name="logger" visibility="private" isLeaf="true" isStatic="true" type="_9RiXk1IkEeibjuLm5_lcNQ" isReadOnly="true" association="_9pMCkFIkEeibjuLm5_lcNQ">
                    <defaultValue xmi:type="uml:LiteralString" xmi:id="_9cV5ZFIkEeibjuLm5_lcNQ" value="Logger.getLogger(CaptureGoodsDialog.class.getName())"/>
