     if (FreeColDebugger.isInDebugMode(FreeColDebugger.DebugMode.MENUS)) {&#xD;
         toolTipText += &quot; &quot; + market.getAmountInMarket(type);&#xD;
     }&#xD;
     setToolTipText(toolTipText);&#xD;
