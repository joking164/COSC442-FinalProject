                      <eAnnotations xmi:id="_9kjaTVIkEeibjuLm5_lcNQ" source="http://www.eclipse.org/uml2/2.0.0/UML">
                        <details xmi:id="_9kjaTlIkEeibjuLm5_lcNQ" key="final"/>
