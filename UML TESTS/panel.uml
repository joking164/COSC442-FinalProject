                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9kJxkFIkEeibjuLm5_lcNQ" name="getMinimapEconomicColor" specification="_9aQ1wFIkEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_9kJxkVIkEeibjuLm5_lcNQ" name="type" type="_9VJFMFIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_9kJxklIkEeibjuLm5_lcNQ" type="_9STzoFIkEeibjuLm5_lcNQ" direction="return"/>
