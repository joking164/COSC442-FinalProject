                  </ownedAttribute>
                  <ownedAttribute xmi:id="_9c-LgFIkEeibjuLm5_lcNQ" name="compassRose" visibility="private" isLeaf="true" type="_9Ri-o1IkEeibjuLm5_lcNQ" isReadOnly="true" association="_9wlrEVIkEeibjuLm5_lcNQ"/>
