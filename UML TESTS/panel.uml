                    <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9jBJI1IkEeibjuLm5_lcNQ" name="add" specification="_9XgctFIkEeibjuLm5_lcNQ">
                      <ownedParameter xmi:id="_9jBJJFIkEeibjuLm5_lcNQ" name="comp" type="_9RpsUFIkEeibjuLm5_lcNQ"/>
                      <ownedParameter xmi:id="_9jBJJVIkEeibjuLm5_lcNQ" name="editState">
                        <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                      </ownedParameter>
                      <ownedParameter xmi:id="_9jBJJlIkEeibjuLm5_lcNQ" type="_9RpsUFIkEeibjuLm5_lcNQ" direction="return"/>
                      <language>java</language>
                      <body>Container oldParent = comp.getParent();&#xD;
         if (editState) {&#xD;
             if (comp instanceof UnitLabel) {&#xD;
                 UnitLabel unitLabel = (UnitLabel)comp;&#xD;
                 Unit unit = unitLabel.getUnit();&#xD;
&#xD;
                 if (!unit.isOnCarrier()) {&#xD;
                     igc().putOutsideColony(unit);&#xD;
                 }&#xD;
&#xD;
                 if (unit.getColony() == null) {&#xD;
                     closeColonyPanel();&#xD;
                     return null;&#xD;
                 } else if (!(unit.getLocation() instanceof Tile)&#xD;
                     &amp;&amp; !unit.isOnCarrier()) {&#xD;
                     return null;&#xD;
                 }&#xD;
&#xD;
                 oldParent.remove(comp);&#xD;
                 initialize();&#xD;
                 return comp;&#xD;
             } else {&#xD;
                 logger.warning(&quot;Invalid component: &quot; + comp);&#xD;
                 return null;&#xD;
             }&#xD;
         } else {&#xD;
             ((UnitLabel)comp).setSmall(false);&#xD;
             return add(comp);&#xD;
         }&#xD;
     </body>
                    </ownedBehavior>
