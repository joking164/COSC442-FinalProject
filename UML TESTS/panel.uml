                    <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9j6hBFIkEeibjuLm5_lcNQ" name="hasUnit" specification="_6Qj55FIkEeibjuLm5_lcNQ">
                      <ownedParameter xmi:id="_9j6hBVIkEeibjuLm5_lcNQ" direction="return">
                        <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
