                  <ownedAttribute xmi:id="_9lXSglIkEeibjuLm5_lcNQ" name="colonyDemandPanel" visibility="private" type="_6Ra1gFIkEeibjuLm5_lcNQ" association="_-AYakVIkEeibjuLm5_lcNQ"/>
                  <ownedAttribute xmi:id="_9lX5kVIkEeibjuLm5_lcNQ" name="goodsDemandPanel" visibility="private" type="_6RcqsFIkEeibjuLm5_lcNQ" association="_-Ad6IVIkEeibjuLm5_lcNQ"/>
                  <ownedAttribute xmi:id="_9lX5lFIkEeibjuLm5_lcNQ" name="inciteDemandPanel" visibility="private" type="_6Ref4FIkEeibjuLm5_lcNQ" association="_-AfIQVIkEeibjuLm5_lcNQ"/>
                  <ownedAttribute xmi:id="_9lYgolIkEeibjuLm5_lcNQ" name="unitDemandPanel" visibility="private" type="_6RixUFIkEeibjuLm5_lcNQ" association="_-AiLkVIkEeibjuLm5_lcNQ"/>
                  <ownedAttribute xmi:id="_9lZHsFIkEeibjuLm5_lcNQ" name="offer" visibility="private" type="_9R3uwVIkEeibjuLm5_lcNQ" association="_-AnrIVIkEeibjuLm5_lcNQ"/>
                  <ownedAttribute xmi:id="_9lZHs1IkEeibjuLm5_lcNQ" name="accept" visibility="private" type="_9SKCoVIkEeibjuLm5_lcNQ" association="_-Au_4VIkEeibjuLm5_lcNQ">
