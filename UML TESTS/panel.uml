                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9jmX9lIkEeibjuLm5_lcNQ" name="FatherDetailPanel" specification="_9ZCGsFIkEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_9jmX91IkEeibjuLm5_lcNQ" name="freeColClient" type="_9Rla4VIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_9jmX-FIkEeibjuLm5_lcNQ" name="colopediaPanel" type="_6OL7UFIkEeibjuLm5_lcNQ"/>
