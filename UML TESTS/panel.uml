                    <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9jV5QFIkEeibjuLm5_lcNQ" name="getAbbreviatedName" specification="_6OxxNFIkEeibjuLm5_lcNQ">
                      <ownedParameter xmi:id="_9jV5QVIkEeibjuLm5_lcNQ" name="name">
                        <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
