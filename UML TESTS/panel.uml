                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_AgXEB1IlEeibjuLm5_lcNQ" name="SelectDestinationDialog" specification="_ACmJ8FIlEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_AgXECFIlEeibjuLm5_lcNQ" name="freeColClient" type="_9Rla4VIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_AgXECVIlEeibjuLm5_lcNQ" name="frame" type="_9SJbkFIkEeibjuLm5_lcNQ"/>
