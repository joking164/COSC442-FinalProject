                  <ownedAttribute xmi:id="_9iKNglIkEeibjuLm5_lcNQ" name="shouldEnable" visibility="private">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                    <defaultValue xmi:type="uml:LiteralString" xmi:id="_9iKNg1IkEeibjuLm5_lcNQ" value="false"/>
