             } else if ((template = newRoute.verify()) == null) {&#xD;
                 igc().updateTradeRoute(newRoute);&#xD;
                 if (u != null) igc().assignTradeRoute(u, newRoute);&#xD;
                 updateList(newRoute);&#xD;
