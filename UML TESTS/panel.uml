                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_AgSykFIlEeibjuLm5_lcNQ" name="setMessages" specification="_ABU-oVIlEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_AgSykVIlEeibjuLm5_lcNQ" name="messages" type="_9RtWsFIkEeibjuLm5_lcNQ"/>
