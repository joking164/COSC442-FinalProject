             if (editable) {&#xD;
                 unitLabel.setTransferHandler(portPanel.getTransferHandler());&#xD;
                 unitLabel.addMouseListener(portPanel.getPressListener());&#xD;
             }&#xD;
             add(unitLabel);&#xD;
