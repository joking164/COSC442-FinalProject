                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_AgOhIFIlEeibjuLm5_lcNQ" name="update" specification="_AAvv0lIlEeibjuLm5_lcNQ">
                    <language>java</language>
                    <body>reportPanel.removeAll();&#xD;
&#xD;
     // Define the layout, with a column for each goods type.&#xD;
     String cols = &quot;[l][c][c][c]&quot;;&#xD;
     for (int i = 0; i &lt; this.goodsTypes.size(); i++) cols += &quot;[c]&quot;;&#xD;
     cols += &quot;[c][c][l][l][l]&quot;;&#xD;
     reportPanel.setLayout(new MigLayout(&quot;fillx, insets 0, gap 0 0&quot;,&#xD;
                                         cols, &quot;&quot;));&#xD;
&#xD;
     conciseHeaders(this.market);&#xD;
     List&lt;ColonySummary> summaries = new ArrayList&lt;>();&#xD;
     for (List&lt;Colony> cs : this.colonies) {&#xD;
         summaries.clear();&#xD;
         for (Colony c : cs) {&#xD;
             ColonySummary s = new ColonySummary(c, this.goodsTypes);&#xD;
             summaries.add(s);&#xD;
             updateColony(s);&#xD;
         }&#xD;
         if (cs.size() > 1) {&#xD;
             updateCombinedColonies(summaries);&#xD;
         }&#xD;
         conciseHeaders(this.market);&#xD;
     }&#xD;
 </body>
                  </ownedBehavior>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_AgOhIVIlEeibjuLm5_lcNQ" name="actionPerformed" specification="_AAvv01IlEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_AgOhIlIlEeibjuLm5_lcNQ" name="ae" type="_9Rhwg1IkEeibjuLm5_lcNQ"/>
                    <language>java</language>
                    <body>final Game game = getGame();&#xD;
     String command = ae.getActionCommand();&#xD;
     if (command.startsWith(BUILDQUEUE)) {&#xD;
         command = command.substring(BUILDQUEUE.length());&#xD;
         Colony colony = game.getFreeColGameObject(command, Colony.class);&#xD;
         if (colony != null) {&#xD;
             getGUI().showBuildQueuePanel(colony, () -> { update(); });&#xD;
             return;&#xD;
         }&#xD;
     } else {&#xD;
         Colony colony = game.getFreeColGameObject(command, Colony.class);&#xD;
         if (colony != null) {&#xD;
             getGUI().showColonyPanel2(colony, null)&#xD;
                 .addClosingCallback(() -> { update(); });&#xD;
             return;&#xD;
         }&#xD;
     }&#xD;
     super.actionPerformed(ae);&#xD;
 </body>
                  </ownedBehavior>
