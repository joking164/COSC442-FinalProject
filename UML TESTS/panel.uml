                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Agltg1IlEeibjuLm5_lcNQ" name="isOnCarrier" specification="__-ee8FIkEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_AglthFIlEeibjuLm5_lcNQ" direction="return">
                      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
