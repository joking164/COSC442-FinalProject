     try (&#xD;
         FileInputStream fis = new FileInputStream(logFile);&#xD;
         BufferedInputStream logFileStream = new BufferedInputStream(fis);&#xD;
     ) {&#xD;
         logFileStream.read(buffer);&#xD;
         message = new String(buffer, &quot;UTF-8&quot;);&#xD;
     } catch (Exception e) {&#xD;
         ;// ignore&#xD;
     }&#xD;
&#xD;
