                    <eAnnotations xmi:id="_AfZatlIlEeibjuLm5_lcNQ" source="http://www.eclipse.org/uml2/2.0.0/UML">
                      <details xmi:id="_AfZat1IlEeibjuLm5_lcNQ" key="ConstantWithGetter"/>
                    </eAnnotations>
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
