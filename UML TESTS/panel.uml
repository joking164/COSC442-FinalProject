                  </ownedComment>
                  <generalization xmi:id="_AA-ZUFIlEeibjuLm5_lcNQ" general="_9YD2VFIkEeibjuLm5_lcNQ"/>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_AgQWVVIlEeibjuLm5_lcNQ" name="ReportReligiousPanel" specification="_AA9yQFIlEeibjuLm5_lcNQ">
