                    <ownedAttribute xmi:id="_9eenZ1IkEeibjuLm5_lcNQ" name="PRODUCTION" visibility="private" isLeaf="true" isStatic="true" isReadOnly="true">
                      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                      <defaultValue xmi:type="uml:LiteralString" xmi:id="_9eenaFIkEeibjuLm5_lcNQ" value="4"/>
