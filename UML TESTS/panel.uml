                    <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9jV5S1IkEeibjuLm5_lcNQ" name="startAnimation" specification="_6OyYRlIkEeibjuLm5_lcNQ">
                      <language>java</language>
                      <body>ActionListener taskPerformer = (ActionEvent ae) -> {&#xD;
             if (counter &lt; points.length - 1) {&#xD;
                 counter += 20;&#xD;
                 if (counter > points.length) {&#xD;
                     counter = points.length - 1;&#xD;
                     ((Timer)ae.getSource()).stop();&#xD;
                     notifyStopped();&#xD;
                 }&#xD;
                 validate();&#xD;
                 repaint();&#xD;
             } else {&#xD;
                 ((Timer)ae.getSource()).stop();&#xD;
                 notifyStopped();&#xD;
             }&#xD;
         };&#xD;
         new Timer(ANIMATION_DELAY, taskPerformer).start();&#xD;
     </body>
                    </ownedBehavior>
                    <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9jV5TFIkEeibjuLm5_lcNQ" name="paintComponent" specification="_9YWxQlIkEeibjuLm5_lcNQ">
