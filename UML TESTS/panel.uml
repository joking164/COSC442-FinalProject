                      <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_9kAAmlIkEeibjuLm5_lcNQ" value="*"/>
                    </ownedParameter>
                    <language>java</language>
                    <body>return player.getSpecification().getUnitTypeList().stream()&#xD;
         .filter(ut -> ut.hasSkill() &amp;&amp; ut.isAvailableTo(player))&#xD;
         .collect(Collectors.toList());&#xD;
 </body>
