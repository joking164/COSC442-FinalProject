             TradeRoute tradeRoute = unit.getTradeRoute();&#xD;
             if (tradeRoute != null) {&#xD;
                 unitLabel.setDescriptionLabel(unit&#xD;
                     .getDescription(Unit.UnitLabelType.NATIONAL)&#xD;
                     + &quot; (&quot; + tradeRoute.getName() + &quot;)&quot;);&#xD;
