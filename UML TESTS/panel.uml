                    <ownedParameter xmi:id="_9kjaQ1IkEeibjuLm5_lcNQ" name="from" type="_9TRc81IkEeibjuLm5_lcNQ">
                      <eAnnotations xmi:id="_9kjaRFIkEeibjuLm5_lcNQ" source="http://www.eclipse.org/uml2/2.0.0/UML">
