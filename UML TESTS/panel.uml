                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_AgaHZlIlEeibjuLm5_lcNQ" name="checkVictoryConditions" specification="_ADL_0FIlEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_AgaHZ1IlEeibjuLm5_lcNQ" direction="return">
                      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                    </ownedParameter>
                    <language>java</language>
                    <body>Specification spec = getSpecification();&#xD;
     if (singlePlayerGame&#xD;
         &amp;&amp; spec.getBoolean(GameOptions.VICTORY_DEFEAT_EUROPEANS)&#xD;
         &amp;&amp; !spec.getBoolean(GameOptions.VICTORY_DEFEAT_REF)) {&#xD;
         int n = 0;&#xD;
         for (Entry&lt;Nation, NationState> e&#xD;
                  : getGame().getNationOptions().getNations().entrySet()) {&#xD;
             if (e.getKey().getType().isEuropean()&#xD;
                 &amp;&amp; !e.getKey().isUnknownEnemy()&#xD;
                 &amp;&amp; e.getValue() != NationState.NOT_AVAILABLE) n++;&#xD;
         }&#xD;
         if (n == 0) {&#xD;
             getGUI().showInformationMessage(&quot;info.noEuropeans&quot;);&#xD;
             return false;&#xD;
         }&#xD;
     }&#xD;
     return true;&#xD;
 </body>
                  </ownedBehavior>
