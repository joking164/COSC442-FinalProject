                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9jnmEFIkEeibjuLm5_lcNQ" name="updateSearch" specification="_9UOfMFIkEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_9jnmEVIkEeibjuLm5_lcNQ" name="displayListOption" type="_6PnesFIkEeibjuLm5_lcNQ"/>
                    <language>java</language>
                    <body>DefaultListModel&lt;Settlement> model&#xD;
         = new DefaultListModel&lt;>();&#xD;
     Object selected = this.settlementList.getSelectedValue();&#xD;
&#xD;
     for (Player player : getGame().getLivePlayers(null)) {&#xD;
         boolean ok;&#xD;
         switch (displayListOption) {&#xD;
         case ONLY_NATIVES:&#xD;
             ok = player.isIndian();&#xD;
             break;&#xD;
         case ONLY_EUROPEAN:&#xD;
             ok = player.isEuropean();&#xD;
             break;&#xD;
         case ALL:&#xD;
             ok = true;&#xD;
             break;&#xD;
         default:&#xD;
             ok = false;&#xD;
             break;&#xD;
         }&#xD;
         if (ok) {&#xD;
             for (Settlement s : player.getSettlements()) {&#xD;
                 model.addElement(s);&#xD;
             }&#xD;
         }&#xD;
     }&#xD;
&#xD;
     this.settlementList.setModel(model);&#xD;
     this.settlementList.setSelectedValue(selected, true);&#xD;
     if (this.settlementList.getSelectedIndex() &lt; 0) {&#xD;
         this.settlementList.setSelectedIndex(0);&#xD;
     }&#xD;
 </body>
                  </ownedBehavior>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9jnmElIkEeibjuLm5_lcNQ" name="selectSettlement" specification="_6PoFwFIkEeibjuLm5_lcNQ">
                    <language>java</language>
                    <body>Settlement settlement = this.settlementList.getSelectedValue();&#xD;
     if (settlement instanceof Colony&#xD;
         &amp;&amp; settlement.getOwner() == getMyPlayer()) {&#xD;
         getGUI().removeFromCanvas(FindSettlementPanel.this);&#xD;
         getGUI().showColonyPanel((Colony)settlement, null);&#xD;
     } else if (settlement instanceof IndianSettlement) {&#xD;
         getGUI().removeFromCanvas(FindSettlementPanel.this);&#xD;
         getGUI().showIndianSettlementPanel((IndianSettlement)settlement);&#xD;
     }&#xD;
 </body>
                  </ownedBehavior>
