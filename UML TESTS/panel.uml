         if (activeUnit == null || !activeUnit.hasTile()&#xD;
             || (!(activeUnit.getLocation() instanceof Tile)&#xD;
                 &amp;&amp; !activeUnit.isOnCarrier())) {&#xD;
             igc().nextActiveUnit();&#xD;
         }&#xD;
     }&#xD;
