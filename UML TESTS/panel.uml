                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9kJxk1IkEeibjuLm5_lcNQ" name="getMinimapPoliticsColor" specification="_9aQ1w1IkEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_9kJxlFIkEeibjuLm5_lcNQ" name="type" type="_9VJFMFIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_9kJxlVIkEeibjuLm5_lcNQ" type="_9STzoFIkEeibjuLm5_lcNQ" direction="return"/>
