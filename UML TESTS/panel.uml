                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9kkBT1IkEeibjuLm5_lcNQ" name="addMarketItems" specification="_9bqj8FIkEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_9kkBUFIkEeibjuLm5_lcNQ" name="ag" type="_9Rn3IVIkEeibjuLm5_lcNQ">
                      <eAnnotations xmi:id="_9kkBUVIkEeibjuLm5_lcNQ" source="http://www.eclipse.org/uml2/2.0.0/UML">
                        <details xmi:id="_9kkBUlIkEeibjuLm5_lcNQ" key="final"/>
                      </eAnnotations>
                    </ownedParameter>
                    <ownedParameter xmi:id="_9kkBU1IkEeibjuLm5_lcNQ" name="europe" type="_9Tjw0FIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_9kkBVFIkEeibjuLm5_lcNQ" direction="return">
                      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                    </ownedParameter>
                    <language>java</language>
                    <body>final InGameController igc = freeColClient.getInGameController();&#xD;
     final Goods goods = new Goods(europe.getGame(), null,&#xD;
                                   ag.getType(), ag.getAmount());&#xD;
     boolean added = false;&#xD;
     for (final Unit unit : europe.getUnitList()) {&#xD;
         if (unit.isCarrier() &amp;&amp; unit.canCarryGoods()&#xD;
             &amp;&amp; unit.canAdd(goods)) {&#xD;
             StringTemplate template = StringTemplate.template(&quot;loadOnTo&quot;)&#xD;
                 .addStringTemplate(&quot;%unit%&quot;,&#xD;
                     unit.getLabel(Unit.UnitLabelType.NATIONAL));&#xD;
             JMenuItem menuItem = Utility.localizedMenuItem(template);&#xD;
             menuItem.addActionListener((ActionEvent ae) -> {&#xD;
                     if ((ae.getModifiers() &amp; ActionEvent.SHIFT_MASK) != 0) {&#xD;
                         promptForGoods(ag);&#xD;
                     }&#xD;
                     igc.buyGoods(ag.getType(), ag.getAmount(), unit);&#xD;
                 });&#xD;
             this.add(menuItem);&#xD;
             added = true;&#xD;
         }&#xD;
     }&#xD;
     return added;&#xD;
 </body>
                  </ownedBehavior>
