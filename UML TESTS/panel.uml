                 if (n == null || n &lt;= 0) {&#xD;
                     setText(name);&#xD;
                 } else {&#xD;
                     setText(name + &quot;  (&quot; + n + &quot;)&quot;);&#xD;
                 }&#xD;
                 return ret;&#xD;
             }&#xD;
         });&#xD;
&#xD;
     JScrollPane tradeRouteView = new JScrollPane(tradeRoutes);&#xD;
