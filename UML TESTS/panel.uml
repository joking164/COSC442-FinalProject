                      </ownedParameter>
                      <ownedParameter xmi:id="_9jitnVIkEeibjuLm5_lcNQ" type="_9RpsUFIkEeibjuLm5_lcNQ" direction="return"/>
                      <language>java</language>
                      <body>if (editState) {&#xD;
             if (!(comp instanceof GoodsLabel)) {&#xD;
                 logger.warning(&quot;Invalid component: &quot; + comp);&#xD;
                 return null;&#xD;
             }&#xD;
&#xD;
             Goods goods = ((GoodsLabel)comp).getGoods();&#xD;
             if (getMyPlayer().canTrade(goods.getType())) {&#xD;
                 igc().sellGoods(goods);&#xD;
             } else {&#xD;
                 BoycottAction act = getGUI()&#xD;
                     .getBoycottChoice(goods, europe);&#xD;
                 if (act != null) {&#xD;
                     switch (act) {&#xD;
                     case PAY_ARREARS:&#xD;
                         igc().payArrears(goods.getType());&#xD;
                         break;&#xD;
                     case DUMP_CARGO:&#xD;
                         igc().unloadCargo(goods, true);&#xD;
                         break;&#xD;
                     default:&#xD;
                         logger.warning(&quot;showBoycottedGoodsDialog fail: &quot;&#xD;
                             + act);&#xD;
                         break;&#xD;
                     }&#xD;
                 }&#xD;
             }&#xD;
             cargoPanel.revalidate();&#xD;
             revalidate();&#xD;
             igc().nextModelMessage();&#xD;
         }&#xD;
         EuropePanel.this.refresh();&#xD;
         return comp;&#xD;
     </body>
                    </ownedBehavior>
