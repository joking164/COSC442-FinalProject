     addMouseListeners();&#xD;
     setTransferHandlers(isEditable());&#xD;
&#xD;
     // Enable/disable widgets&#xD;
     unloadButton.addActionListener(this);&#xD;
     fillButton.addActionListener(this);&#xD;
