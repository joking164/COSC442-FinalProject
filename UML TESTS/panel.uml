                    <ownedParameter xmi:id="_AgVO1VIlEeibjuLm5_lcNQ" name="freeColClient" type="_9Rla4VIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_AgVO1lIlEeibjuLm5_lcNQ" name="frame" type="_9SJbkFIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_AgVO11IlEeibjuLm5_lcNQ" name="goodsType" type="_9Rn3I1IkEeibjuLm5_lcNQ"/>
