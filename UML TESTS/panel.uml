                  <ownedAttribute xmi:id="_Ae6Sh1IlEeibjuLm5_lcNQ" name="comboBox" visibility="private" isLeaf="true" type="_9SVo1FIkEeibjuLm5_lcNQ" isReadOnly="true" association="_AkFGYVIlEeibjuLm5_lcNQ"/>
