                <packagedElement xmi:type="uml:Class" xmi:id="_ADxOoFIlEeibjuLm5_lcNQ" name="TilePanel" isLeaf="true">
                  <ownedComment xmi:id="_Ag2zR1IlEeibjuLm5_lcNQ">
                    <body>This panel is used to show information about a tile.</body>
                  </ownedComment>
