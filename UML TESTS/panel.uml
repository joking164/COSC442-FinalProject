                      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9jFakVIkEeibjuLm5_lcNQ" name="updateDescriptionLabel" specification="_9XoYhlIkEeibjuLm5_lcNQ">
                        <ownedParameter xmi:id="_9jFaklIkEeibjuLm5_lcNQ" name="unitLabel" type="_9XEX0FIkEeibjuLm5_lcNQ"/>
                        <ownedParameter xmi:id="_9jFak1IkEeibjuLm5_lcNQ" name="toAdd">
                          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                        </ownedParameter>
                        <language>java</language>
                        <body>String tileMsg = Messages.message(colonyTile.getLabel());&#xD;
             if (unitLabel == null) {&#xD;
                 setToolTipText(tileMsg);&#xD;
             } else {&#xD;
                 String unitMsg = unitLabel.getUnit()&#xD;
                     .getDescription(Unit.UnitLabelType.NATIONAL);&#xD;
                 if (toAdd) unitMsg = tileMsg + &quot; [&quot; + unitMsg + &quot;]&quot;;&#xD;
                 unitLabel.setDescriptionLabel(unitMsg);&#xD;
             }&#xD;
         </body>
