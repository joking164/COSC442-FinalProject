                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_AgcjnlIlEeibjuLm5_lcNQ" name="displayStatsMessage" specification="_ADZbNVIlEeibjuLm5_lcNQ">
                    <ownedParameter xmi:id="_Agcjn1IlEeibjuLm5_lcNQ" name="title">
                      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                    </ownedParameter>
                    <ownedParameter xmi:id="_AgcjoFIlEeibjuLm5_lcNQ" name="stats" type="_9RtWslIkEeibjuLm5_lcNQ"/>
                    <ownedParameter xmi:id="_AgcjoVIlEeibjuLm5_lcNQ" type="_9RwaAVIkEeibjuLm5_lcNQ" direction="return"/>
                    <language>java</language>
                    <body>JPanel panel = new JPanel();&#xD;
     panel.setBorder(Utility.localizedBorder(title));&#xD;
     Box b = new Box(BoxLayout.Y_AXIS);&#xD;
     panel.add(b);&#xD;
     Map&lt;String, String> memory = new HashMap&lt;>();&#xD;
     Map&lt;String, String> ai = new HashMap&lt;>();&#xD;
     for (String k : memoryKeys) {&#xD;
         memory.put(Messages.message(&quot;memoryManager.&quot; + k),&#xD;
                    stats.remove(k));&#xD;
     }&#xD;
     for (String k : new ArrayList&lt;>(stats.keySet())) {&#xD;
         if (k.startsWith(&quot;AI&quot;)) { // FIXME: AIMain.aiStatisticsPrefix&#xD;
             ai.put(k, stats.remove(k));&#xD;
         }&#xD;
     }&#xD;
     b.add(createStatsTable(&quot;Memory&quot;, memory));&#xD;
     b.add(createStatsTable(&quot;Game&quot;, stats));&#xD;
     if (ai.isEmpty()) {&#xD;
         b.add(new JLabel());&#xD;
     } else {&#xD;
         b.add(createStatsTable(&quot;AI&quot;, ai));&#xD;
     }&#xD;
     return panel;&#xD;
 </body>
                  </ownedBehavior>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_AgcjolIlEeibjuLm5_lcNQ" name="createStatsTable" specification="_ADdFkFIlEeibjuLm5_lcNQ">
