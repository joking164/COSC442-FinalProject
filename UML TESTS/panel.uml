                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_AgXrFlIlEeibjuLm5_lcNQ" name="updateDestinationComparator" specification="_ACqbYFIlEeibjuLm5_lcNQ">
                    <language>java</language>
                    <body>final Player player = getMyPlayer();&#xD;
     switch (this.comparatorBox.getSelectedIndex()) {&#xD;
     case 1:&#xD;
         this.destinationComparator = new NameComparator(player);&#xD;
         break;&#xD;
     case 2:&#xD;
         this.destinationComparator = new DistanceComparator(player);&#xD;
         break;&#xD;
     case 0: default:&#xD;
         this.destinationComparator = new DestinationComparator(player);&#xD;
         break;&#xD;
     }&#xD;
 </body>
                  </ownedBehavior>
