                <ownedParameter xmi:id="_G0nXsVIiEeibjuLm5_lcNQ" type="_Gp0c8FIiEeibjuLm5_lcNQ" direction="return"/>
                <language>java</language>
                <body>super(FreeCol.CLIENT_THREAD+&quot;Worker&quot;);&#xD;
     jobList = new LinkedBlockingQueue&lt;>();&#xD;
     stopRunning = false;&#xD;
 </body>
              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_G0nXslIiEeibjuLm5_lcNQ" name="run" specification="_Gp1rFFIiEeibjuLm5_lcNQ">
                <language>java</language>
                <body>while (!stopRunning) {&#xD;
         try {&#xD;
             // run the next waiting job&#xD;
             Runnable job = jobList.take();&#xD;
             try {&#xD;
                 job.run();&#xD;
             } catch (Exception e) {&#xD;
                 logger.log(Level.SEVERE, &quot;Worker task failed!&quot;, e);&#xD;
             }&#xD;
         } catch (InterruptedException e) {&#xD;
             logger.log(Level.INFO, &quot;Worker interrupted, aborting!&quot;, e);&#xD;
         }&#xD;
     }&#xD;
 </body>
