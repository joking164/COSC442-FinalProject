                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Xs5KN1InEeibjuLm5_lcNQ" name="summarizeSettlement" specification="_XsjL4VInEeibjuLm5_lcNQ">
                  <ownedParameter xmi:id="_Xs5KOFInEeibjuLm5_lcNQ" name="freeColClient" type="_Xr7g0VInEeibjuLm5_lcNQ">
                    <eAnnotations xmi:id="_Xs5KOVInEeibjuLm5_lcNQ" source="http://www.eclipse.org/uml2/2.0.0/UML">
