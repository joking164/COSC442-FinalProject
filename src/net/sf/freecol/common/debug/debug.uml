                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Xs3U8FInEeibjuLm5_lcNQ" name="addBuildings" specification="_XsZa4FInEeibjuLm5_lcNQ">
                  <ownedParameter xmi:id="_Xs3U8VInEeibjuLm5_lcNQ" name="freeColClient" type="_Xr7g0VInEeibjuLm5_lcNQ">
