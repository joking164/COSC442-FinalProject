                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_jM-3cFJOEeiyW7NaN4qWdw" name="Playlist" specification="_jMg9YFJOEeiyW7NaN4qWdw">
                  <ownedParameter xmi:id="_jM-3cVJOEeiyW7NaN4qWdw" name="soundFiles" type="_jLxWgFJOEeiyW7NaN4qWdw">
