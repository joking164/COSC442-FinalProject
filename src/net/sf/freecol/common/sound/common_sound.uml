                    </ownedParameter>
                    <language>java</language>
                    <body>String input;&#xD;
         for (;;) {&#xD;
             switch (oggSyncState.pageout(oggPage)) { &#xD;
             case 0:&#xD;
                 if ((input = getInput()) != null) return input;&#xD;
                 break;&#xD;
             case 1:&#xD;
                 oggStreamState.pagein(oggPage);&#xD;
                 return null;&#xD;
             default:&#xD;
                 return &quot;Bogus page&quot;;&#xD;
             }&#xD;
         }&#xD;
