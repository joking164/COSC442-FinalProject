                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_jM8bPFJOEeiyW7NaN4qWdw" name="decodePacket" specification="_jJVGgFJOEeiyW7NaN4qWdw">
                    <ownedParameter xmi:id="_jM8bPVJOEeiyW7NaN4qWdw" name="samples">
