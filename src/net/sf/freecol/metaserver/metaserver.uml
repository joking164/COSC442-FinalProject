              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_1ZfrMFJOEeiyW7NaN4qWdw" name="getItem" specification="_1VLL4FJOEeiyW7NaN4qWdw">
                <ownedParameter xmi:id="_1ZfrMVJOEeiyW7NaN4qWdw" name="address">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
