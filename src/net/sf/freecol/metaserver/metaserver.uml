              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_1ZfrNFJOEeiyW7NaN4qWdw" name="indexOf" specification="_1VLy8lJOEeiyW7NaN4qWdw">
                <ownedParameter xmi:id="_1ZfrNVJOEeiyW7NaN4qWdw" name="address">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                </ownedParameter>
                <ownedParameter xmi:id="_1ZfrNlJOEeiyW7NaN4qWdw" name="port">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                </ownedParameter>
                <ownedParameter xmi:id="_1ZfrN1JOEeiyW7NaN4qWdw" direction="return">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                </ownedParameter>
                <language>java</language>
                <body>for (int i=0; i&lt;items.size(); i++) {&#xD;
         if (address.equals(items.get(i).getAddress()) &amp;&amp; port == items.get(i).getPort()) {&#xD;
             return i;&#xD;
         }&#xD;
     }&#xD;
&#xD;
     return -1;&#xD;
 </body>
              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_1ZfrOFJOEeiyW7NaN4qWdw" name="removeDeadServers" specification="_1VMaAVJOEeiyW7NaN4qWdw">
                <language>java</language>
                <body>logger.info(&quot;Removing dead servers.&quot;);&#xD;
&#xD;
     long time = System.currentTimeMillis() - MetaServer.REMOVE_OLDER_THAN;&#xD;
     for (int i=0; i&lt;items.size(); i++) {&#xD;
         if (items.get(i).getLastUpdated() &lt; time) {&#xD;
             logger.info(&quot;Removing: &quot; + items.get(i));&#xD;
             items.remove(i);&#xD;
         }&#xD;
     }&#xD;
 </body>
              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_1ZgSQFJOEeiyW7NaN4qWdw" name="addServer" specification="_1VMaBFJOEeiyW7NaN4qWdw">
                <ownedParameter xmi:id="_1ZgSQVJOEeiyW7NaN4qWdw" name="name">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                </ownedParameter>
                <ownedParameter xmi:id="_1ZgSQlJOEeiyW7NaN4qWdw" name="address">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                </ownedParameter>
                <ownedParameter xmi:id="_1ZgSQ1JOEeiyW7NaN4qWdw" name="port">
