     Timer t = new Timer(true);&#xD;
     t.scheduleAtFixedRate(new TimerTask() {&#xD;
         @Override&#xD;
         public void run() {&#xD;
             try {&#xD;
                 mr.removeDeadServers();&#xD;
             } catch (Exception ex) {&#xD;
                 logger.log(Level.WARNING, &quot;Could not remove servers.&quot;, ex);&#xD;
