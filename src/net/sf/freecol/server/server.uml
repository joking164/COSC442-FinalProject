              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_D8L4k1JPEeiyW7NaN4qWdw" name="createEmptyMap" specification="_D7zeEFJPEeiyW7NaN4qWdw">
                <ownedParameter xmi:id="_D8L4lFJPEeiyW7NaN4qWdw" name="game" type="_D66tQVJPEeiyW7NaN4qWdw"/>
                <ownedParameter xmi:id="_D8L4lVJPEeiyW7NaN4qWdw" name="width">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                </ownedParameter>
