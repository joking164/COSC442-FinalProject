              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_D8LRhVJPEeiyW7NaN4qWdw" name="readGame" specification="_D7waxFJPEeiyW7NaN4qWdw">
                <ownedParameter xmi:id="_D8LRhlJPEeiyW7NaN4qWdw" name="file" type="_D6rcsVJPEeiyW7NaN4qWdw"/>
                <ownedParameter xmi:id="_D8LRh1JPEeiyW7NaN4qWdw" name="spec" type="_D6--sVJPEeiyW7NaN4qWdw"/>
                <ownedParameter xmi:id="_D8LRiFJPEeiyW7NaN4qWdw" name="server" type="_D0fVIFJPEeiyW7NaN4qWdw"/>
                <ownedParameter xmi:id="_D8LRiVJPEeiyW7NaN4qWdw" type="_D7LzAVJPEeiyW7NaN4qWdw" direction="return"/>
                <language>java</language>
                <body>ServerGame g = null;&#xD;
     try {&#xD;
         g = FreeColServer.readGame(new FreeColSavegameFile(file),&#xD;
                                    spec, server);&#xD;
         logger.info(&quot;Imported file &quot; + file.getPath());&#xD;
     } catch (Exception e) {&#xD;
         logger.log(Level.WARNING, &quot;Import failed for &quot; + file.getPath(), e);&#xD;
     }&#xD;
&#xD;
     // If importing as a result of &quot;Start Game&quot; in the map editor,&#xD;
     // consume the file.&#xD;
     File startGame = FreeColDirectories.getStartMapFile();&#xD;
     if (startGame != null&#xD;
         &amp;&amp; startGame.getPath().equals(file.getPath())) {&#xD;
         file.delete();&#xD;
     }&#xD;
     return g;&#xD;
 </body>
